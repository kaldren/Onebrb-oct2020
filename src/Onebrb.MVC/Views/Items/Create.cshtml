@using Microsoft.AspNetCore.Mvc.Localization
@model Onebrb.MVC.Models.Item.CreateItemViewModel
@inject IViewLocalizer Localizer

@if (User.Identity.IsAuthenticated)
{
    string currentUserId = this.User.Claims.FirstOrDefault(x => x.Type == "Id").Value;
    <component type="typeof(Onebrb.MVC.Components.Items.CreateItem)" param-Model="Model" param-CurrentUserId="currentUserId" render-mode="ServerPrerendered" />
}


@{
}

@section Scripts {
    <script src="_framework/blazor.server.js"></script>
}